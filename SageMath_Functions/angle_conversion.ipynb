   "source": [
    "# Convert radian to degree.\n",
    "def rad2deg(rad):\n",
    "    '''Convert radian to degree'''\n",
    "    deg = (180 / pi) * rad\n",
    "    return deg\n",
    "\n",
    "# Convert degree to radian.\n",
    "def deg2rad(deg):\n",
    "    '''Convert degree to radian'''\n",
    "    rad = (pi / 180) * deg\n",
    "    return rad"
   ]
  }
